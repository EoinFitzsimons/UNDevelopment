                <Component class="javax.swing.JTextArea" name="explainGameDisplay">
                  <Properties>
                    <Property name="background" type="java.awt.Color" editor="org.netbeans.beaninfo.editors.ColorEditor">
                      <Color blue="cd" green="e1" red="e5" type="rgb"/>
